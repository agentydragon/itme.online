{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "~": "./src",
          "~components": "./src/components",
          "~lib": "./src/lib",
          "~modules": "./src/modules",
          "~spaces": "./src/spaces",
          "~flows": "./src/flows",
          "~hooks": "./src/hooks",
          "@solid/lit-pod": "@itme/lit-pod"
        }
      }
    ]
  ]
}
